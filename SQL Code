use store;
select * from customers
order by first_name

SELECT first_name, last_name, points, points + 10
from customers

SELECT first_name, last_name, points, (points * 10) + 100
from customers

SELECT first_name, last_name, points, ((points + 10)*100) AS DISCOUNT_FACTOR
from customers

Select first_name, Last_name, points, (points * 1.1) AS New_Price from customers

select * customer
where birthdate > 1991-01-

select birth_date from customers
where birth_date > 1991-01-01
